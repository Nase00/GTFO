{
  "name": "gtfo-api",
  "version": "0.0.1",
  "description": "Gently Tell Folks Out of meeting rooms - remote unit control",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:seanowiecki/gtfo-api.git"
  },
  "main": "application.js",
  "scripts": {
    "start": "npm run prod",
    "clean": "rimraf server/public/dist",
    "build": "webpack",
    "lint": "eslint --ext .js,.jsx client server",
    "server": "node ./application.js",
    "dev": "npm run clean && nf start dev-server,dev-client --",
    "hot": "npm run clean && HOT=true npm run dev-server --",
    "prod": "npm run clean && npm run prod-client && npm run prod-server --",
    "dev-client": "webpack-dev-server --config webpack.config.dev.js --color --progress --inline --content-base build",
    "dev-server": "nodemon --ignore mock-data.json --ignore client/ ./application.js",
    "prod-client": "npm run lint && NODE_ENV=production webpack --config webpack.config.prod.js",
    "prod-server": "NODE_ENV=production npm run server",
    "deploy-server": "NODE_ENV=production forever start ./application.js",
    "yolo": "rimraf mock-data.json",
    "test": "npm run clean && npm run lint"
  },
  "author": "Sean Owiecki",
  "contributors": [
    {
      "name": "Sean Owiecki",
      "email": "sean.owiecki@slalom.com"
    }
  ],
  "license": "none",
  "dependencies": {
    "body-parser": "^1.15.0",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "foreman": "^1.4.1",
    "immutable": "^3.7.6",
    "immutable-props": "^1.0.0",
    "johnny-five": "^0.9.24",
    "lodash": "^4.5.0",
    "material-ui": "^0.14.4",
    "moment": "^2.11.2",
    "morgan": "^1.7.0",
    "particle-io": "^0.12.0",
    "radium": "^0.16.5",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.1",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-addons-transition-group": "^0.14.1",
    "react-addons-update": "^0.14.1",
    "react-dom": "^0.14.7",
    "react-motion": "^0.4.2",
    "react-redux": "^3.1.0",
    "react-router": "2.0.0",
    "react-router-redux": "^3.0.0",
    "react-swipeable-views": "^0.4.1",
    "react-tap-event-plugin": "^0.2.1",
    "recompose": "^0.15.0",
    "redux": "^3.3.1",
    "serve-favicon": "^2.3.0",
    "slug": "^0.9.1",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.3",
    "ws": "^1.0.1",
    "yargs": "^4.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.7",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-react": "^3.16.1",
    "file-loader": "^0.8.5",
    "nodemon": "^1.8.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "react-transform-render-visualizer": "^0.4.0",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.4.3",
    "svg-react-loader": "^0.3.2",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.12.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
