{
  "name": "gtfo",
  "version": "1.2.0",
  "description": "Gently Tell Folks Out of meeting rooms - remote unit control",
  "repository": {
    "type": "git",
    "url": "git@github.com:Nase00/gtfo.git"
  },
  "main": "application.js",
  "node_test_paths": "./tests/server/**/*.spec.* ./tests/universal/**/*.spec.*",
  "scripts": {
    "flash": "node firmware/flash",
    "clean": "rimraf server/public/dist",
    "demo": "node environment/demo",
    "deploy-server": "NODE_ENV=production forever start ./application.js",
    "dev-server": "nodemon --ignore mock-data.json --ignore client/ --ignore /tests ./application.js",
    "hot": "npm run clean && HOT=true npm run dev-server --",
    "lint": "eslint --ext .js,.jsx *.js client server universal environment tests webpack karma",
    "prod": "npm run clean && npm run prod-client && npm run prod-server --",
    "prod-client": "NODE_ENV=production webpack --config webpack/webpack.prod.config.js",
    "prod-server": "NODE_ENV=production npm run server",
    "server": "node ./application.js",
    "start": "npm run prod",
    "test": "npm run clean && npm run lint && npm run test-node && npm run test-client && snyk test",
    "test-client": "karma start karma/karma.conf",
    "test-client-cov": "karma start karma/karma.cov.conf.js",
    "test-cov": "npm run test-client-cov && npm run test-node-cov && npm run upload-cov && rm -rf ./karma/coverage",
    "test-node": "NODE_ENV=test mocha --exit $npm_package_node_test_paths --require @babel/register --require tests/config.node.js --reporter=dot",
    "test-node-cov": "nyc --report lcovonly --require babel-core/register --require ./tests/config.node.js npm run test-node",
    "upload-cov": "cat ./coverage/*/lcov.info ./coverage/lcov.info | coveralls",
    "yolo": "rimraf environment/mock-data.json",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "Sean Owiecki",
  "contributors": [
    {
      "name": "Sean Owiecki",
      "email": "seanowiecki@gmail.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": "^10.4.1",
    "npm": "^6.1.0"
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-loader": "^8.0.2",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "blessed": "^0.1.81",
    "blessed-contrib": "4.8.5",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.0",
    "express": "^4.15.2",
    "file-loader": "^1.1.5",
    "history": "^4.6.1",
    "immutable": "^3.7.6",
    "json-loader": "^0.5.4",
    "jsonschema": "^1.1.0",
    "lodash": "4.17.5",
    "material-ui": "0.20.0",
    "moment": "2.20.1",
    "morgan": "^1.7.0",
    "particle-api-js": "^7.0.1",
    "prop-types": "15.6.0",
    "query-string": "^5.0.1",
    "radium": "0.22.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-motion": "0.5.2",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-swipeable-views": "0.12.12",
    "recompose": "0.26.0",
    "redux": "^4.0.0",
    "rimraf": "^2.5.2",
    "serve-favicon": "^2.3.0",
    "slugify": "^1.1.0",
    "split": "^1.0.0",
    "velocity-animate": "^1.5.0",
    "velocity-react": "1.4.0",
    "webpack": "4.23.1",
    "webpack-cli": "3.1.2",
    "ws": "4.1.0",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-eslint": "^10.0.1",
    "chromedriver": "^2.28.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.0.0",
    "eslint-plugin-filenames": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "expect": "^21.2.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-source-map-support": "1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.5",
    "nyc": "^11.3.0",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^4.3.11",
    "sinon": "^4.1.3",
    "snyk": "^1.19.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "snyk": true
}
