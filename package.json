{
  "name": "gtfo-api",
  "version": "0.0.1",
  "description": "Gently Tell Folks Out of meeting rooms - remote unit control",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:seanowiecki/gtfo-api.git"
  },
  "main": "application.js",
  "scripts": {
    "start": "npm run prod",
    "clean": "rimraf public",
    "build": "webpack",
    "lint": "eslint --ext .js,.jsx client server",
    "dev-server": "nodemon --ignore mock-data.json --ignore client/ ./application.js",
    "server": "node ./application.js",
    "dev": "npm run clean && nf start dev-server,dev-client",
    "dev-mocks": "npm run clean && MOCKS=true nf start dev-server,dev-client",
    "hot": "npm run clean && HOT=true npm run dev-server",
    "hot-dd": "npm run clean && HOT=true DISABLE_DEVICES=true npm run dev-server",
    "hot-mocks": "npm run clean && HOT=true MOCKS=true npm run dev-server",
    "hot-mocks-dd": "npm run clean && HOT=true MOCKS=true DISABLE_DEVICES=true npm run dev-server",
    "prod": "npm run clean && npm run prod-client && npm run prod-server",
    "dev-client": "webpack-dev-server --config webpack.config.dev.js --color --progress --inline --content-base build",
    "prod-client": "npm run lint && NODE_ENV=production webpack --config webpack.config.prod.js",
    "prod-server": "NODE_ENV=production npm run server",
    "deploy-server": "NODE_ENV=production forever start ./application.js",
    "yolo": "rm mock-data.json",
    "test": "npm run lint"
  },
  "author": "Sean Owiecki",
  "contributors": [
    {
      "name": "Sean Owiecki",
      "email": "sean.owiecki@slalom.com"
    }
  ],
  "license": "none",
  "dependencies": {
    "body-parser": "^1.14.1",
    "chart.js": "^1.0.2",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.0",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "foreman": "^1.4.1",
    "history": "1.13.1",
    "immutable": "^3.7.6",
    "immutable-props": "^1.0.0",
    "johnny-five": "^0.9.17",
    "lodash": "^3.10.1",
    "material-ui": "^0.14.1",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "particle-io": "^0.12.0",
    "radium": "^0.16.5",
    "react": "^0.14.2",
    "react-addons-css-transition-group": "^0.14.1",
    "react-addons-pure-render-mixin": "^0.14.1",
    "react-addons-transition-group": "^0.14.1",
    "react-addons-update": "^0.14.1",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "react-router": "1.0.2",
    "react-select": "^0.7.0",
    "react-tap-event-plugin": "^0.2.1",
    "react-tools": "^0.10.0",
    "recompose": "^0.14.5",
    "redux": "^3.0.5",
    "serve-favicon": "^2.3.0",
    "slug": "^0.9.1",
    "temporal": "^0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.7",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.21.0",
    "eslint": "^1.8.0",
    "eslint-config-defaults": "^7.0.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.5.1",
    "fast-csv": "^0.6.0",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.1",
    "nodemon": "^1.8.0",
    "react-transform": "0.0.3",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "svg-react-loader": "^0.3.2",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.0",
    "webpack-hot-middleware": "^2.4.1"
  }
}
