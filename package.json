{
  "name": "gtfo",
  "version": "1.2.0",
  "description": "Gently Tell Folks Out of meeting rooms - remote unit control",
  "repository": {
    "type": "git",
    "url": "git@github.com:Nase00/gtfo.git"
  },
  "main": "application.js",
  "node_test_paths": "./tests/server/**/*.spec.* ./tests/universal/**/*.spec.*",
  "scripts": {
    "flash": "node firmware/flash",
    "clean": "rimraf server/public/dist",
    "demo": "node environment/demo",
    "deploy-server": "NODE_ENV=production forever start ./application.js",
    "dev-server": "nodemon --ignore mock-data.json --ignore client/ --ignore /tests ./application.js",
    "hot": "yarn run clean && HOT=true yarn run dev-server --",
    "lint": "eslint --ext .js,.jsx *.js client server universal environment tests webpack karma",
    "prod": "yarn run clean && yarn run prod-client && yarn run prod-server --",
    "prod-client": "NODE_ENV=production webpack --config webpack/webpack.prod.config.js",
    "prod-server": "NODE_ENV=production yarn run server",
    "server": "node ./application.js",
    "start": "yarn run prod",
    "test": "yarn run clean && yarn run lint && yarn run test-node && yarn run test-client && snyk test",
    "test-client": "karma start karma/karma.conf",
    "test-client-cov": "karma start karma/karma.cov.conf.js",
    "test-cov": "yarn run test-client-cov && yarn run test-node-cov && yarn run upload-cov && rm -rf ./karma/coverage",
    "test-node": "NODE_ENV=test mocha $npm_package_node_test_paths --compilers js:babel-core/register --require tests/config.node.js --reporter=dot",
    "test-node-cov": "istanbul cover mocha $npm_package_node_test_paths --report lcovonly -- --compilers js:babel-core/register --require tests/config.node.js --reporter=dot",
    "upload-cov": "cat ./coverage/*/lcov.info ./coverage/lcov.info | coveralls",
    "yolo": "rimraf environment/mock-data.json",
    "snyk-protect": "snyk protect",
    "prepublish": "yarn run snyk-protect"
  },
  "author": "Sean Owiecki",
  "contributors": [
    {
      "name": "Sean Owiecki",
      "email": "seanowiecki@gmail.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": "^8.9.3",
    "npm": "^5.5.1"
  },
  "private": true,
  "dependencies": {
    "app-module-path": "^2.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.3.13",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.6.5",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.0",
    "express": "^4.15.2",
    "file-loader": "^1.1.5",
    "history": "^4.6.1",
    "immutable": "^3.7.6",
    "imports-loader": "^0.7.0",
    "johnny-five": "^0.11.7",
    "json-loader": "^0.5.4",
    "jsonschema": "^1.1.0",
    "lodash": "^4.5.1",
    "material-ui": "^0.20.0",
    "moment": "^2.16.0",
    "morgan": "^1.7.0",
    "particle-api-js": "^7.0.1",
    "particle-io": "^0.14.0",
    "prop-types": "^15.5.2",
    "query-string": "^5.0.1",
    "radium": "^0.19.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-swipeable-views": "^0.12.12",
    "recompose": "^0.26.0",
    "redux": "^3.5.2",
    "rimraf": "^2.5.2",
    "serve-favicon": "^2.3.0",
    "slugify": "^1.1.0",
    "split": "^1.0.0",
    "velocity-animate": "^1.5.0",
    "velocity-react": "^1.3.3",
    "webpack": "^3.10.2",
    "ws": "^3.3.2",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "app-module-path": "^2.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.3",
    "babel-preset-react-hmre": "^1.1.0",
    "chromedriver": "^2.28.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.0.0",
    "eslint-plugin-filenames": "^1.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "expect": "^21.2.1",
    "imports-loader": "^0.7.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-source-map-support": "1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.3",
    "mocha": "^4.0.1",
    "nightwatch": "^0.9.14",
    "nodemon": "^1.8.0",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "3.1.3",
    "sinon": "^4.1.3",
    "snyk": "^1.19.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.9.7",
    "webpack-hot-middleware": "^2.21.0"
  },
  "snyk": true
}
